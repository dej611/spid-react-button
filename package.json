{
  "name": "@dej611/spid-react-button",
  "version": "0.3.1",
  "description": "Pulsante SSO per SPID in React",
  "author": "dej611",
  "license": "EUPL-1.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dej611/spid-react-button.git"
  },
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "sideEffects": false,
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build:ts": "parcel build",
    "build:svg": "./scripts/svgs.sh",
    "build:css": "./scripts/styles.sh",
    "build": "npm run build:css && npm run build:svg && npm run build:ts && npm run check && npm run doc",
    "start": "parcel watch",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom --coverage",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "check": "tsc --noEmit",
    "doc": "typedoc src/index.tsx --excludePrivate --json ./doc.json && node scripts/doc.js"
  },
  "peerDependencies": {
    "@types/react": ">=16.8.0",
    "react": ">=16.8.0",
    "typeface-titillium-web": "latest"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "typeface-titillium-web": {
      "optional": true
    }
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.2.1",
    "@parcel/transformer-typescript-types": "^2.2.1",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@testing-library/user-event": "13.0.6",
    "@types/jest": "^25.1.4",
    "@types/jest-axe": "^3.5.1",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@types/react-transition-group": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "jest-axe": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.2.1",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "spid-smart-button": "1.1.5",
    "svgo": "^2.2.2",
    "typedoc": "0.21.9",
    "typeface-titillium-web": "latest",
    "typescript": "4.4.4"
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/dej611/spid-react-button/issues"
  },
  "homepage": "https://github.com/dej611/spid-react-button",
  "dependencies": {
    "react-focus-on": "^3.5.2"
  },
  "keywords": [
    "spid",
    "login",
    "sso",
    "italia.it"
  ],
  "jest": {
    "moduleNameMapper": {
      "/../(.*).svg$": "<rootDir>/src/$1.svg",
      "/./(.*).svg$": "<rootDir>/src/shared/$1.svg"
    }
  },
  "bundlewatch": {
    "trackBranches": [
      "main"
    ],
    "files": [
      {
        "path": "./dist/index.js",
        "maxSize": "7.6 KB"
      },
      {
        "path": "./dist/index.modern.js",
        "maxSize": "7.5 KB"
      }
    ]
  }
}